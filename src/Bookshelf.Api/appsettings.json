{
  "Search": {
    "FantLab": {
      "Enabled": true,
      "BaseUrl": "https://api.fantlab.ru",
      "SearchPath": "/search",
      "QueryParameter": "query",
      "TimeoutSeconds": 8,
      "MaxRetries": 2,
      "RetryDelayMilliseconds": 300,
      "CacheTtlMinutes": 5,
      "CircuitBreakerFailureThreshold": 3,
      "CircuitBreakerOpenSeconds": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}

{
  "Search": {
    "FantLab": {
      "Enabled": true,
      "BaseUrl": "https://api.fantlab.ru",
      "SearchPath": "/search",
      "QueryParameter": "query",
      "TimeoutSeconds": 8,
      "MaxRetries": 2,
      "RetryDelayMilliseconds": 300,
      "CacheTtlMinutes": 5,
      "CircuitBreakerFailureThreshold": 3,
      "CircuitBreakerOpenSeconds": 60
    }
  },
  "Downloads": {
    "Jackett": {
      "Enabled": false,
      "UseMockFallback": true,
      "BaseUrl": "http://localhost:9117",
      "ApiKey": "",
      "Indexer": "all",
      "TimeoutSeconds": 8,
      "MaxItems": 20,
      "MaxRetries": 2,
      "RetryDelayMilliseconds": 300
    },
    "Qbittorrent": {
      "Enabled": false,
      "UseMockFallback": true,
      "BaseUrl": "http://localhost:8080",
      "Username": "",
      "Password": "",
      "TimeoutSeconds": 8,
      "MockAutoCompleteSeconds": 4,
      "MaxRetries": 2,
      "RetryDelayMilliseconds": 300
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}

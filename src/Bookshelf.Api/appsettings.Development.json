{
  "Search": {
    "FantLab": {
      "Enabled": true,
      "BaseUrl": "https://api.fantlab.ru",
      "SearchPath": "/search",
      "QueryParameter": "query",
      "TimeoutSeconds": 5,
      "MaxRetries": 1,
      "RetryDelayMilliseconds": 200,
      "CacheTtlMinutes": 2,
      "CircuitBreakerFailureThreshold": 2,
      "CircuitBreakerOpenSeconds": 30
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
